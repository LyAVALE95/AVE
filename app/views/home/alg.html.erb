            <p>Bodegas/Fábricas: <br/>{{data}}
            </p>
            <!--<label>Método para la medición de costos de transporte en base a la capacidad de entrega y solicitud.</label>-->
            
                <input class="btnD btn btn-info"  type="button" value="+Bodega" ng-click="addColumn()"/>
                <input class="btnD btn btn-info" type="button" value="+Fábrica" ng-click="addRow()"/>
 <br>
 <div id = "Pleccion"> </div>
