  <input class="btnD3 btn btn-primary"  type="button" style="visibility: {{showD3}};" ng-click="doRedo()" value="Volver a realizar este ejercicio"/>
                <table id="DataFab" class=" table-bordered">
                <thead>
  				<tr>
    			</tr>
  				</thead>
                    <tr ng-repeat="datum in data">
                        <td width="120px"> <button style="visibility: {{showD}}; width: {{widthD}}" class="btnD btn btn-danger" data-ng-click="removeRow($index)">Eliminar fábrica</button></td>
                        <th width="50px"> F{{$index}}: </th>
                        <td ng-repeat="field in datum">
                          	<input type="text" style="background-color: {{field.cellcolor}}; width: 100px;"
                             ng-change="{{metodoinput}}"
                             ng-model="field.valor"/>
                  			<button width="120px" style="visibility: {{showD}}; width: {{widthD}}" class="btnD btn btn-danger" ng-click="removeColumn($index)">Eliminar Bodega</button>
                  		 </td>
                    </tr>
                </table>
                	<br>
                <input class="btnDP btn btn-success" type="button"  value="Hecho" onclick="completeTableJS()" ng-click="completeTable()"/>
                <input class="btnD3 btn btn-warning" type="button"  style="visibility: {{showD2}};" value="Regresar" ng-click="goBack()"/>
               <!-- <input class="btnD2 btn btn-success"  type="button" onclick="completeTableJS()" value="Continuar"/>-->
