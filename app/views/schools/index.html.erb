<p id="notice"><%= notice %></p>

<h1>Schools</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Principal name</th>
      <th>Type</th>
      <th>Grade</th>
      <th>Code</th>
      <th>Phone</th>
      <th>Web page</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schools.each do |school| %>
      <tr>
        <td><%= school.name %></td>
        <td><%= school.location %></td>
        <td><%= school.principal_name %></td>
        <td><%= school.typeSchool %></td>
        <td><%= school.grade %></td>
        <td><%= school.code %></td>
        <td><%= school.phone %></td>
        <td><%= school.web_page %></td>
        <td><%= link_to 'Show', school %></td>
        <td><%= link_to 'Edit', edit_school_path(school) %></td>
        <td><%= link_to 'Destroy', school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School', new_school_path %>
