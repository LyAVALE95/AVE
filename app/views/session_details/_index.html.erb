<p id="notice"><%= notice %></p>

<h5>Lecciones</h5>

<table>
  <thead>
    <tr>
      <th>Título</th>
      <th>Descripción</th>
      <th>Contenido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @session_details.each do |session_detail| %>
      <tr>
        <td><%= session_detail.txt1 %></td>
        <td><%= session_detail.txt2 %></td>
        <td><%= session_detail.txt3 %></td>
        <td><%= link_to 'Mostrar', session_detail %></td>
        <td><%= link_to 'Editar', edit_session_detail_path(session_detail) %></td>
        <td><%= link_to 'Eliminar', session_detail, method: :delete, data: { confirm: '¿Está seguro(a)?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session Detail', new_session_detail_path %>