<p id="notice"><%= notice %></p>

<h1>Amethods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Subject</th>
      <th>Description</th>
      <th>Description subject</th>
      <th>Skills</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @amethods.each do |amethod| %>
      <tr>
        <td><%= amethod.name %></td>
        <td><%= amethod.subject %></td>
        <td><%= amethod.description %></td>
        <td><%= amethod.description_subject %></td>
        <td><%= amethod.skills %></td>
        <td><%= link_to 'Show', amethod %></td>
        <td><%= link_to 'Edit', edit_amethod_path(amethod) %></td>
        <td><%= link_to 'Destroy', amethod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Amethod', new_amethod_path %>
