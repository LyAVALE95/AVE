<body class="fixed-background-body" style="background-image: url('/backgrounds/photo-1463936575829-25148e1db1b8.jpg");'>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="card cardround-g">
    <div class="card-content">
      <h5>Ingresar</h5>
    </div>
    <div class="card-tabs">
      <ul class="tabs tabs-fixed-width">
        <li class="tab"><a href="#test4">Opciones</a></li>
        <li class="tab"><a class="active" href="#test5">Ingreso</a></li>
      </ul>
    </div>
    <div class="card-content grey lighten-4">
      <div id="test4"><a href="/users/sign_up">Registrarse</a></div>
      <div id="test5">  
      <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <center><%= f.submit "Log in", class:'btn-danger' %></center>
  </div>
  </div>
    </div>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</body>
